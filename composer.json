{
    "name": "mirko-pagliai/me-cms-link-scanner",
    "description": "Link Scanner plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-link-scanner",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "^3.6",
        "mirko-pagliai/me-cms": "^2.25",
        "mirko-pagliai/cakephp-link-scanner": "^1.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "MeCmsLinkScanner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsLinkScanner\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    },
    "minimum-stability": "beta"
}

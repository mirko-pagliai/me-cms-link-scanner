{
    "name": "mirko-pagliai/me-cms-link-scanner",
    "description": "Link Scanner plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-link-scanner",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }],
    "require": {
        "php": ">=7.0",
        "mirko-pagliai/me-cms": "^2.27",
        "mirko-pagliai/cakephp-link-scanner": "^1.0.5"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0|^4.0",
        "mirko-pagliai/cakephp-assets": "^1.4.1",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "MeCmsLinkScanner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsLinkScanner\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    }
}

{
    "name": "mirko-pagliai/me-cms-link-scanner",
    "description": "Link Scanner plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-link-scanner",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }],
    "require": {
        "php": ">=7.2",
        "ctlabvn/recaptcha": "dev-cake-4",
        "dereuromark/cakephp-feed": "^2.0|2.0.0-beta",
        "mirko-pagliai/me-cms": "dev-cakephp4",
        "mirko-pagliai/cakephp-link-scanner": "^1.1"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ctlabvn/recaptcha",
                "version": "dev-cake-4",
                "source": {
                    "url": "https://github.com/ctlabvn/recaptcha.git",
                    "type": "git",
                    "reference": "cake-4"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "MeCmsLinkScanner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsLinkScanner\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    }
}
